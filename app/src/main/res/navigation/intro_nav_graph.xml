<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/intro_nav_graph"
    app:startDestination="@id/introductionFragment">
    <fragment
        android:id="@+id/introductionFragment"
        android:name="com.example.manshatsoultancommunity.features.Intro.fragment.IntroductionFragment"
        android:label="IntroductionFragment">
        <action
            android:id="@+id/action_introductionFragment_to_optionLoginFragment"
            app:destination="@id/optionLoginFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right" />
    </fragment>
    <fragment
        android:id="@+id/optionLoginFragment"
        android:name="com.example.manshatsoultancommunity.features.Intro.fragment.OptionLoginFragment"
        android:label="OptionLoginFragment" />
</navigation>